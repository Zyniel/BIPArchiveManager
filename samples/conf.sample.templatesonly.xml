<?xml version="1.0" encoding="UTF-8"?>
<Settings>
	<!--
		Unarchiver settings.
		7zip compliant only for the moment.
	-->
	<ZipSettings>
		<!-- Path to the 7zip executable -->
		<Path>C:\Program Files\7-Zip\7z.exe</Path>
	</ZipSettings>
	
	<!--
		Root application settings.
	-->
	<AppSettings>
		<Certs>B</Certs>
	</AppSettings>
	
	<!--
		Default parametrized arguments
	-->
	<Arguments>
		<!-- Default source folder path -->
		<InputDirPath>C:\Temp\AA\in</InputDirPath>
		<!-- Default output folder path -->
		<OutputDirPath>C:\Temp\AA\out</OutputDirPath>
		<!-- Default temporary folder path -->
		<WorkDirPath>C:\Temp\AA\tmp</WorkDirPath>
		<!-- -->
		<OmitThumbnails>true</OmitThumbnails>
		<!-- -->
		<OmitDataSamples>true</OmitDataSamples>
		<!-- -->
		<OmitSecurity>true</OmitSecurity>
		<!-- -->
		<OmitMetadata>true</OmitMetadata>
		<!-- -->
		<OmitReports>false</OmitReports>
		<!-- -->
		<OmitDataModels>true</OmitDataModels>
		<!-- -->
		<OmitSubtemplates>true</OmitSubtemplates>
		<!-- -->
		<OmitTranslations>true</OmitTranslations>
		<!-- -->
		<OmitStyleTemplates>true</OmitStyleTemplates>
		<!-- -->
		<ExtractSQL>false</ExtractSQL>	
	</Arguments>
	
	<!--
		Oracle BI Publisher SOAP Endpoints to target
	-->
	<EndPoints>
		<!-- DEV BI Publisher Enpoint -->
		<EndPoint active="true">http://bipublisher.dev.intra.corp/xmlpserver</EndPoint>
		<!-- INT BI Publisher Enpoint -->
		<EndPoint active="true">http://bipublisher.int.intra.corp/xmlpserver</EndPoint>
		<!-- UAT BI Publisher Enpoint -->
		<EndPoint active="true">http://bipublisher.uat.intra.corp/xmlpserver</EndPoint>
		<!-- PRE BI Publisher Enpoint -->
		<EndPoint active="true">http://bipublisher.pre.intra.corp/xmlpserver</EndPoint>
		<!-- PRD BI Publisher Enpoint -->
		<EndPoint active="true">http://bipublisher.intra.corp/xmlpserver</EndPoint>	
	</EndPoints>
</Settings>


